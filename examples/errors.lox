fun divideNumbers(a, b) {
    if (b == 0) {
        print "Cannot divide by zero";
        return nil;
    }
    return a / b;
}

print divideNumbers(10, 2);  // 5
print divideNumbers(10, 0);  // Cannot divide by zero

